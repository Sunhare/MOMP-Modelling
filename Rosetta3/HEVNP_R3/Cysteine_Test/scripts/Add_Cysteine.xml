<ROSETTASCRIPTS>
    <SCOREFXNS>
        <ScoreFunction name="r15_sym" weights="ref2015" symmetric="true" />
    </SCOREFXNS>
    <RESIDUE_SELECTORS>
        <True name="full_pose" />
    </RESIDUE_SELECTORS>
    <TASKOPERATIONS>
        <ReadResfile name="add_cys" filename="inputs/add_cys_resfile.txt" selector="full_pose" />
        <RestrictToRepacking name="nodesign" />
    </TASKOPERATIONS>
    <FILTERS>
        <!-- <ScoreType name="total_score" scorefxn="r15" score_type="total_score" threshold="0" /> -->
    </FILTERS>
    <MOVERS>
        <SetupForSymmetry name="dimer_sym" definition="Dimer_2zzq.symdef" />
        <SymPackRotamersMover name="symm_pack" scorefxn="r15_sym" task_operations="add_cys" />
        <SymMinMover name="symm_min" scorefxn="r15_sym" bb="false" chi="true" jump="ALL" />
    </MOVERS>
    <APPLY_TO_POSE>
    </APPLY_TO_POSE>
    <PROTOCOLS>
        <Add mover="dimer_sym" />
        <Add mover="symm_pack" />
        <Add mover="symm_min" />

    </PROTOCOLS>
    <OUTPUT scorefxn="r15_sym" />
</ROSETTASCRIPTS>
